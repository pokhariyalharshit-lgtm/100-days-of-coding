#include <stdio.h>
#include <limits.h>

int main(void) {
    long long n;
    if (scanf("%lld", &n) != 1) return 1;

    if (n < 2) {
        // No even numbers in range 1..n
        printf("Product=0\n");
        return 0;
    }

    unsigned long long prod = 1ULL;
    int found = 0;
    for (long long i = 2; i <= n; i += 2) {
        found = 1;
        if (prod > ULLONG_MAX / (unsigned long long)i) {
            // avoid silent overflow
            printf("Overflow\n");
            return 0;
        }
        prod *= (unsigned long long)i;
    }

    if (!found) printf("Product=0\n");
    else printf("Product=%llu\n", prod);
    return 0;
}
